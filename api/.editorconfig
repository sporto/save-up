# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[*.rs]
indent_style = tab
indent_size = 4

[*.tera]
indent_style = tab
indent_size = 4

[*.elm]
indent_style = space
indent_size = 4

[*.ex]
indent_style = space
indent_size = 2

[*.ts]
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab
